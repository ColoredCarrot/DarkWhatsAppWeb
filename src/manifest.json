{
  "manifest_version": 2,
  "name": "Dark WhatsApp Web",
  "version": "1.0",
  "description": "Dark Mode for WhatsApp Web",
  "homepage_url": "https://github.com/ColoredCarrot/DarkWhatsAppWeb",
  "author": "Voidev",
  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}